{
  "expo": {
    "name": "BlockFinaX",
    "slug": "blockfinax-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "blockfinax",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "BlockFinaX needs camera access for video calls",
        "NSMicrophoneUsageDescription": "BlockFinaX needs microphone access for voice and video calls",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.blockfinax.mobile"
    },
    "android": {
      "package": "com.blockfinax.mobile",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "extra": {
      "socketUrl": "https://server-production-2529.up.railway.app",
      "eas": {
        "projectId": "60b13dc5-0cbd-4ce7-ae96-df50363d3d05"
      }
    },
    "owner": "blockfinax"
  }
}
